#include <stdio.h>
int main() {
    char s[] = "hellooo";
	char d[20];
    printf("source: %s\n", s);
    printf("dest before: %p\n", d);
    memmove(&d[0],&s[2],2);
    printf("dest after: %s\n", d);
	printf("source after: %s\n", s);
    return (0);
}